{
  "expo": {
    "name": "suremarket",
    "slug": "suremarket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_android.png",
    "splash": {
      "image": "./assets/blackbg.png",
       "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.classima.radiustheme"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.classima.radiustheme",
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "MEDIA_LIBRARY",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS"
      ],
      "versionCode": 1
    }
  }
}